	public static String getVlan(final String campo, List<String> parametros) {
		String a = null;
		String b = null;
		String c = null;
		if(campo == "c"){
			b = campo;
			c = campo;
			
			parametros.add(b);
			parametros.add(c);
		} else if(campo == "e"){
			a = campo;
			parametros.add(a);

		}
		if (2 <= parametros.size()) {
			return parametros.get(0) + parametros.get(1) + parametros.get(2); 
		}
		
		return null;
		
	}
	
	
	
}
