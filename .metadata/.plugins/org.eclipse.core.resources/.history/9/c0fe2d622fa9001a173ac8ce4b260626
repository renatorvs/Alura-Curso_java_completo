package teste;

public class principal {
	
	
	public static void main(String[] args) {
		
		String[] log = new String[4];
		
		log[1] = "a";
		log[2] = "b";
		log[3] = "c";
		log[4] = "d";
		log[5] = "e";
		
		
		
		
		
		String parametros = null;
		for (String campo : log) {
	 parametros	=	getVlan(campo);
		}
		
		
		
		
		
		
	}

	
	
	
	
	
	
	
	public String getVlan(String campo) {
		String a = null;
		String b = null;
		String c = null;
		if(campo.contentEquals("c")){
			b = campo;
			c = campo;
		} else if(campo.contentEquals("e")){
			a = campo;
		}
		return  "a:" + a + "b:" + b + "c:" + c;
		
	}
	
	
	
}
